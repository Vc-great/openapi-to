{
  "name": "@openapi-to/core",
  "version": "2.4.0-alpha.1",
  "description": "Generator core",
  "keywords": [
    "typescript",
    "plugins",
    "codegen"
  ],
  "repository": {
    "type": "git",
    "url": "",
    "directory": "packages/core"
  },
  "license": "MIT",
  "author": "",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.cjs"
    },
    "./utils": {
      "import": "./dist/utils.js",
      "require": "./dist/utils.cjs",
      "default": "./dist/utils.cjs"
    },
    "./package.json": "./package.json",
    "./*": "./*"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "src",
    "dist",
    "*.d.ts",
    "*.d.cts",
    "schema.json",
    "!/**/**.test.**",
    "!/**/__tests__/**"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "npx rimraf ./dist",
    "lint": "bun biome lint .",
    "lint:fix": "bun biome lint --write --unsafe .",
    "release": "pnpm publish --no-git-check",
    "release:canary": "bash ../../.github/canary.sh && node ../../scripts/build.js canary && pnpm publish --no-git-check -tag canary",
    "release:alpha": "bash ../../.github/canary.sh && node ../../scripts/build.js alpha && pnpm publish --no-git-check -tag alpha",
    "start": "tsup --watch",
    "test": "vitest --passWithNoTests",
    "typecheck": "tsc -p ./tsconfig.json --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@types/json-schema": "^7.0.15",
    "axios": "^1.6.7",
    "code-block-writer": "^12.0.0",
    "consola": "^3.2.3",
    "directory-tree": "^3.5.1",
    "do-swagger2openapi": "^7.0.18",
    "find-up": "^7.0.0",
    "fs-extra": "^11.2.0",
    "is-chinese": "^2.0.0",
    "is-url": "^1.2.4",
    "js-runtime": "^0.0.7",
    "json-schema": "^0.4.0",
    "lodash": "^4.17.21",
    "natural-orderby": "^3.0.2",
    "oas": "^24.9.0",
    "picocolors": "^1.0.0",
    "pinyin-pro": "^3.25.0",
    "seedrandom": "^3.0.5",
    "semver": "^7.5.4",
    "tinyrainbow": "^1.2.0",
    "ts-morph": "^23.0.0"
  },
  "devDependencies": {
    "@openapi-to/config-ts": "workspace:*",
    "@openapi-to/config-tsup": "workspace:*",
    "@types/fs-extra": "^11.0.4",
    "@types/is-url": "^1.2.32",
    "@types/lodash": "^4.17.9",
    "@types/seedrandom": "^3.0.8",
    "@types/semver": "^7.5.6",
    "ora": "^7.0.1"
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=8.3.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
