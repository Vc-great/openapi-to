{
  "name": "openapi-to",
  "version": "1.1.0",
  "description": "根据 OpenAPI规范接口文档生成js,ts代码。",
  "main": "dist/index.js",
  "bin": {
    "openapi": "dist/index.js"
  },
  "type": "module",
  "scripts": {
    "dev": "rimraf dist && rollup -c rollup.config.mjs -w",
    "build": "rimraf dist && rollup -c rollup.config.mjs",
    "test-watch": "jest --watch",
    "test": "jest --coverage --verbose -u",
    "format": "prettier --write \"./**/*.{ts,js,json,md}\"",
    "lint": "eslint . --ext .js,.ts --fix ",
    "prepare": "husky install",
    "build:publish": "jest --verbose -u && rimraf output coverage && pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Vc-great/openapi-to.git"
  },
  "homepage": "https://github.com/Vc-great/openapi-to",
  "author": "",
  "keywords": [
    "swagger",
    "openapi",
    "typescript",
    "javascript"
  ],
  "devDependencies": {
    "@commitlint/config-conventional": "^17.6.6",
    "@rollup/plugin-json": "^6.0.0",
    "@types/assert": "^1.5.6",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.4.1",
    "@types/swagger2openapi": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "assert": "^2.0.0",
    "commitlint": "^17.6.6",
    "esbuild": "^0.18.10",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest": "^27.2.2",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.3",
    "openapi-types": "^12.1.3",
    "prettier-eslint": "^15.0.1",
    "prettier-plugin-organize-imports": "^3.2.2",
    "prettier-plugin-packagejson": "^2.4.3",
    "rimraf": "^5.0.1",
    "rollup": "^3.25.3",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-typescript2": "^0.35.0",
    "ts-jest": "^29.1.0",
    "tslib": "^2.6.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@types/fs-extra": "^11.0.1",
    "@types/node-fetch": "^2.6.4",
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "do-swagger2openapi": "^7.0.14",
    "fs-extra": "^11.1.1",
    "lodash": "^4.17.21",
    "node-fetch": "^2.0.0",
    "prettier": "^2.8.8"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write",
      "eslint --fix"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  }
}
